{
  "name": "@kubeploy/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "setup-tenant": "nest start --entryFile setup-tenant",
    "dev": "nest start --watch | pino-pretty",
    "dev:debug": "nest start --debug --watch",
    "repl": "nest start --entryFile repl",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up && node dist/setup-tenant.js",
    "migration:down": "mikro-orm migration:down",
    "seeder:create": "mikro-orm seeder:create",
    "seeder:run": "mikro-orm seeder:run"
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@casl/ability": "^6.7.3",
    "@kubernetes/client-node": "^1.4.0",
    "@mikro-orm/core": "^6.6.1",
    "@mikro-orm/migrations": "^6.6.1",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.6.1",
    "@mikro-orm/reflection": "^6.6.1",
    "@mikro-orm/seeder": "^6.6.1",
    "@nest-boot/auth": "^7.7.1",
    "@nest-boot/auth-rls": "^7.2.1",
    "@nest-boot/bullmq": "^7.0.0",
    "@nest-boot/graphql": "^7.1.0",
    "@nest-boot/graphql-connection": "^7.2.0",
    "@nest-boot/logger": "^7.0.2",
    "@nest-boot/middleware": "^7.2.0",
    "@nest-boot/mikro-orm": "^7.2.1",
    "@nest-boot/mikro-orm-request-transaction": "^7.1.2",
    "@nest-boot/redis": "^7.0.0",
    "@nest-boot/request-context": "^7.4.0",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/platform-express": "^11.1.9",
    "better-auth": "^1.4.5",
    "bullmq": "^5.65.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dataloader": "^2.2.3",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.8.2",
    "lodash": "^4.17.21",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.5",
    "sonyflake-js": "^1.0.1",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@mikro-orm/cli": "^6.6.1",
    "@nest-boot/eslint-config": "^7.0.1",
    "@nest-boot/eslint-plugin": "^7.0.2",
    "@nest-boot/tsconfig": "^7.0.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
